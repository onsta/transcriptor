{
  "name": "trint",
  "version": "0.0.1",
  "scripts": {
    "dev": "nodemon server/index.ts",
    "build": "next build && tsc --project tsconfig.server.json",
    "start": "NODE_ENV=production node production-server/index.js",
    "compile": "tsc",
    "test": "jest"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "**/*.test.(ts|tsx)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx"
    ]
  },
  "dependencies": {
    "@blueprintjs/icons": "^2.1.1",
    "express": "^4.16.3",
    "isomorphic-unfetch": "^2.0.0",
    "next": "^5.1.0",
    "ramda": "^0.25.0",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-transition-group": "^2.2.1",
    "styled-jsx": "^2.2.6"
  },
  "devDependencies": {
    "@blueprintjs/core": "^2.0.0-rc.3",
    "@types/express": "^4.11.1",
    "@types/jest": "^22.2.0",
    "@types/moment": "^2.13.0",
    "@types/next": "^2.4.8",
    "@types/ramda": "^0.25.21",
    "@types/react": "^16.1.0",
    "@types/react-dom": "^16.0.4",
    "@types/react-transition-group": "^2.0.7",
    "@types/styled-jsx": "^2.2.2",
    "@zeit/next-typescript": "^0.1.1",
    "jest": "^22.4.2",
    "nodemon": "^1.12.1",
    "prop-types": "^15.6.1",
    "ts-jest": "^22.4.2",
    "ts-node": "^5.0.1",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2"
  },
  "license": "ISC"
}
